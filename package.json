{
	"name": "@codeeditorland/extension-recommender",
	"version": "0.0.4",
	"description": "Deep learning model to recommend the best extension for a given VS Code workspace",
	"homepage": "https://github.com/microsoft/vscode-extension-recommender#readme",
	"bugs": {
		"url": "https://github.com/microsoft/vscode-extension-recommender/issues"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/microsoft/vscode-extension-recommender.git"
	},
	"license": "MIT",
	"author": "Harald Kirschner",
	"main": "dist/lib/index.js",
	"module": "dist/lib/index.js",
	"types": "dist/index.d.ts",
	"scripts": {
		"build": "npm run clean && webpack --mode production",
		"clean": "rimraf dist",
		"prepack": "npm run build",
		"prepublishOnly": "npm test",
		"pretest": "npm run build",
		"test": "mocha",
		"watch": "npm run clean && webpack --watch"
	},
	"mocha": {
		"extension": [
			"ts"
		],
		"require": "ts-node/register",
		"spec": "test/*.test.ts",
		"timeout": "5000"
	},
	"dependencies": {},
	"devDependencies": {
		"@types/chai": "^4.3.3",
		"@types/mocha": "8.2.3",
		"@types/node": "^16.11.58",
		"chai": "^4.3.6",
		"copy-webpack-plugin": "^11.0.0",
		"esbuild": "^0.12.29",
		"mocha": "9.2.2",
		"node-fetch": "^2.6.7",
		"npm-run-all": "^4.1.5",
		"onnxruntime-web": "^1.12.1",
		"rimraf": "3.0.2",
		"terser-webpack-plugin": "^5.3.6",
		"ts-loader": "^9.3.1",
		"ts-node": "^10.9.1",
		"typescript": "^4.8.3",
		"webpack": "~5.44.0",
		"webpack-cli": "^4.10.0"
	}
}
