	},
	"devDependencies": {
		"@types/chai": "^4.3.3",
		"@types/node": "^16.11.58",
		"chai": "^4.3.6",
		"esbuild": "^0.12.29",
		"node-fetch": "^2.6.7",
		"npm-run-all": "^4.1.5",
		"onnxruntime-web": "^1.12.1",
